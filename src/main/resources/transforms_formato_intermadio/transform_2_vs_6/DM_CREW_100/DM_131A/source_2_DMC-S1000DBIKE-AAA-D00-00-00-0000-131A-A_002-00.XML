	<content>
		<acrw>
			<frc>
				<title>Pre-ride Inspection</title>
				<drill>
					<title>Brakes</title>
					<subdrill>
						<title>Pads</title>
						<step>
							<challrsp>
								<challeng>
									<para>Pads</para>
								</challeng>
								<response>
									<para>Free of unwanted material</para>
								</response>
							</challrsp>
						</step>
						<step>
							<challrsp>
								<challeng>
									<para>Pads</para>
								</challeng>
								<response>
									<para>Acceptable pad width</para>
								</response>
							</challrsp>
						</step>
						<step>
							<challrsp>
								<challeng>
									<para>Pads</para>
								</challeng>
								<response>
									<para>Acceptable pad clearance</para>
								</response>
							</challrsp>
						</step>
					</subdrill>
					<subdrill>
						<title>Callipers</title>
						<step>
							<challrsp>
								<challeng>
									<para>Link Wire</para>
								</challeng>
								<response>
									<para>Firmly attached</para>
								</response>
							</challrsp>
						</step>
					</subdrill>
					<subdrill>
						<title>Levers</title>
						<step>
							<challrsp>
								<challeng>
									<para>Levers</para>
								</challeng>
								<response>
									<para>Approximately 1 inch of travel before engagement</para>
								</response>
							</challrsp>
						</step>
						<step>
							<challrsp>
								<challeng>
									<para>Levers</para>
								</challeng>
								<response>
									<para>Space between lever and handlebar when fully pulled</para>
								</response>
							</challrsp>
						</step>
					</subdrill>
					<subdrill>
						<title>Cables</title>
						<step>
							<challrsp>
								<challeng>
									<para>Cables</para>
								</challeng>
								<response>
									<para>No cuts or fraying</para>
								</response>
							</challrsp>
						</step>
					</subdrill>
				</drill>
				<drill>
					<title>Tires</title>
					<step>
						<challrsp>
							<challeng>
								<para>Pressure</para>
							</challeng>
							<response>
								<table>
									<tgroup cols="3">
										<colspec colnum="1" colname="COL1" colwidth="*"/>
										<colspec colnum="2" colname="COL2" colwidth="*"/>
										<colspec colnum="3" colname="COL3" colwidth="*"/>
										<thead>
											<row>
												<entry colname="COL1">Tire Pressures</entry>
												<entry colname="COL2">Min</entry>
												<entry colname="COL3">Max</entry>
											</row>
										</thead>
										<tbody>
											<row>
												<entry colname="COL1">Off Road</entry>
												<entry colname="COL2">35lbs</entry>
												<entry colname="COL3">40lbs</entry>
											</row>
											<row>
												<entry colname="COL1">On Road</entry>
												<entry colname="COL2">55lbs</entry>
												<entry colname="COL3">60lbs</entry>
											</row>
										</tbody>
									</tgroup>
								</table>
							</response>
						</challrsp>
					</step>
					<step>
						<challrsp>
							<challeng>
								<para>Tires</para>
							</challeng>
							<response>
								<para>No cracks or splits</para>
							</response>
						</challrsp>
					</step>
				</drill>
				<drill>
					<title>Wheels</title>
					<step>
						<challrsp>
							<challeng>
								<para>Wheels</para>
							</challeng>
							<response>
								<para>No loose bearings</para>
							</response>
						</challrsp>
					</step>
					<step>
						<challrsp>
							<challeng>
								<para>Wheels</para>
							</challeng>
							<response>
								<para>True</para>
							</response>
						</challrsp>
					</step>
					<step>
						<challrsp>
							<challeng>
								<para>Spokes</para>
							</challeng>
							<response>
								<para>Not broken</para>
							</response>
						</challrsp>
					</step>
					<if>
						<condit>Spokes not broken</condit>
						<step>
							<challrsp>
								<challeng>
									<para>Spokes</para>
								</challeng>
								<response>
									<para>Tight</para>
								</response>
							</challrsp>
						</step>
					</if>
					<step>
						<challrsp>
							<challeng>
								<para>Axel Nuts</para>
							</challeng>
							<response>
								<para>Tight</para>
							</response>
						</challrsp>
					</step>
				</drill>
				<drill>
					<title>Headset</title>
					<step>
						<challrsp>
							<challeng>
								<para>Headset bearings</para>
							</challeng>
							<response>
								<para>Tight</para>
							</response>
						</challrsp>
					</step>
				</drill>
				<drill>
					<title>Chain</title>
					<step>
						<challrsp>
							<challeng>
								<para>Links</para>
							</challeng>
							<response>
								<para>Easy movement of links</para>
							</response>
						</challrsp>
					</step>
				</drill>
				<drill>
					<title>Handlebar</title>
					<warning>
						<para>Do not ride with a cracked stem</para>
					</warning>
					<if>
						<condit>Stem cracked</condit>
						<step>
							<procd>
								<para>Replace stem</para>
							</procd>
						</step>
					</if>
					<elseif>
						<condit>Stem is loose</condit>
						<step>
							<procd>
								<para>Tighten stem</para>
							</procd>
						</step>
					</elseif>
					<if>
						<condit>Handlebars twist in stem</condit>
						<step>
							<procd>
								<para>Tighten clamp bolt</para>
							</procd>
						</step>
					</if>
				</drill>
				<drill>
					<title>Computer</title>
					<step>
						<challrsp>
							<challeng>
								<para>Computer Display</para>
							</challeng>
							<response>
								<para>
									<capgrp cols="2">
										<capbody>
											<caprow>
												<capentry>
													<caption colour="co04" level="00001" mark="1" change="add">
														<capline>SPEED</capline>
													</caption>
												</capentry>
												<capentry>
													<caption colour="co51" level="00001" mark="1" change="add">
														<capline>0 mph</capline>
													</caption>
												</capentry>
											</caprow>
											<caprow>
												<capentry>
													<caption colour="co04" level="00001" mark="1" change="add">
														<capline>DISTANCE</capline>
													</caption>
												</capentry>
												<capentry>
													<caption colour="co51" level="00001" mark="1" change="add">
														<capline>0 miles</capline>
													</caption>
												</capentry>
											</caprow>
										</capbody>
									</capgrp>
								</para>
							</response>
						</challrsp>
					</step>
				</drill>
			</frc>
		</acrw>
	</content>
