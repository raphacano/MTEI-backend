		<afi>
			<afi-proc>
				<fault fcode="NYCJD04"/>
				<describe>
					<fdesc>Tire does not function correctly</fdesc>
				</describe>
				<isoproc>
					<prelreqs>
						<reqconds>
							<noconds/>
						</reqconds>
						<supequip>
							<supeqli>
								<supequi id="seq-0001">
									<nomen>Tire pressure gauge</nomen>
									<identno>
										<mfc>KZ666</mfc>
										<pnr>BSK-TLST-001-01</pnr>
									</identno>
									<qty uom="EA">1</qty>
								</supequi>
								<supequi>
									<nomen>Specialist toolset</nomen>
									<identno>
										<mfc>KZ666</mfc>
										<pnr>BSK-TLST-001</pnr>
									</identno>
									<qty uom="EA">1</qty>
								</supequi>
							</supeqli>
						</supequip>
						<supplies>
							<nosupply/>
						</supplies>
						<spares>
							<nospares/>
						</spares>
						<safety>
							<nosafety/>
						</safety>
					</prelreqs>
					<isolatep>
						<isostep id="istp-0001">
							<action>Use the tire pressure gauge (<xref xrefid="seq-0001" xidtype="supequip" xlink:actuate="onRequest" xlink:show="replace" xlink:href="seq-0001"/>)
to do a check of the pressure</action>
							<question>What is the tire pressure reading?</question>
							<answer>
								<sel-list>
									<choice refid="istp-0002">More than 2700 hPa</choice>
									<choice refid="istp-0003">Between 100 hPa and 2700 hPa</choice>
									<choice refid="istp-0004">Less than 100 hPa</choice>
								</sel-list>
							</answer>
						</isostep>
						<isoend id="istp-0002">
							<action>Deflate the tire until the pressure is 2700 hPa</action>
						</isoend>
						<isoend id="istp-0003">
							<action>Inflate the tire as given in
<refdm xlink:type="simple" xlink:actuate="onRequest" xlink:show="replace" xlink:href="URN:S1000D:DMC-S1000DBIKE-AAA-DA0-10-20-00AA-215A-A">
									<avee>
										<modelic>S1000DBIKE</modelic>
										<sdc>AAA</sdc>
										<chapnum>DA0</chapnum>
										<section>1</section>
										<subsect>0</subsect>
										<subject>20</subject>
										<discode>00</discode>
										<discodev>AA</discodev>
										<incode>215</incode>
										<incodev>A</incodev>
										<itemloc>A</itemloc>
									</avee>
								</refdm>
							</action>
						</isoend>
						<isostep id="istp-0004">
							<action>To do a check of the tire for damage</action>
							<question>Is there damage to the tire?</question>
							<answer>
								<yesno>
									<yes refid="istp-0005"/>
									<no refid="istp-0006"/>
								</yesno>
							</answer>
						</isostep>
						<isoend id="istp-0005">
							<action>Replace the tire (refer to
<refdm xlink:type="simple" xlink:actuate="onRequest" xlink:show="replace" xlink:href="URN:S1000D:DMC-S1000DBIKE-AAA-DA0-10-20-00AA-921A-A">
